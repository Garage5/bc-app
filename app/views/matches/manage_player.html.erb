<div id="manage_player" class="modal">
		<div class="modal_hd">
			<a class="modal_close" href=""><img src="/images/icon_close.gif" /></a>
			<h1>Manage Player: <%= @match.send(@player).login %></h1>
		</div><!-- / modal_hd -->
	<div class="modal_inner">
			<div class="clear">
			<%= button_link_to 'Disqualify', disqualify_player_tournament_match_path(@tournament, @match, :p => @player), '', :method => :put %>
			<%= button_link_to 'Declare Winner', declare_winner_tournament_match_path(@tournament, @match, :p => @player), '', :method => :put %>
			<%= button_link_to 'Revert', '#' %>
			</div>
	</div><!-- / modal_inner -->
</div><!-- / modal -->